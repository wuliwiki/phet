{
  "name": "center-and-variability",
  "version": "1.0.0-dev.13",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/center-and-variability.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "CENTER_AND_VARIABILITY",
    "simulation": true,
    "runnable": true,
    "generatedUnitTests": true,
    "supportedBrands": [
      "phet",
      "phet-io",
      "adapted-from-phet"
    ],
    "phetLibs": [
      "bamboo",
      "twixt",
      "vegas"
    ],
    "supportsOutputJS": true,
    "simFeatures": {
      "supportsInteractiveDescription": true,
      "supportsDynamicLocale": true
    }
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js",
    "rules": {
      "todo-should-have-issue": "off"
    }
  }
}