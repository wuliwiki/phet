{
  "name": "projectile-motion",
  "version": "1.1.0-dev.39",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/projectile-motion.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "PROJECTILE_MOTION",
